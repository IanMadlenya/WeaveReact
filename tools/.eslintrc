{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0
  },
  "settings": {
    "ecmascript": 6
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "destructuring": true,
    "classes": true,
    "forOf": true,
    "blockBindings": true,
    "arrowFunctions": true
  }
}
